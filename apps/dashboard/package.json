{
  "name": "@white-label/dashboard",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "vite build && vite build --ssr",
    "dev": "vite dev",
    "format": "prettier --write .",
    "generate-schema": "ts-node --files -r dotenv/config ./app/database/generate-schema.ts ",
    "lint": "eslint --ext .ts,.tsx .",
    "lint-fix": "eslint --ext .ts,.tsx . --fix",
    "mgr-create": "dbmate -d './app/database/migrations' new",
    "mgr-rollback": "dbmate -d './app/database/migrations' rollback",
    "mgr-run": "dbmate -d './app/database/migrations' migrate",
    "mgr-status": "dbmate -d './app/database/migrations' status",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@effect/schema": "^0.44.0",
    "@heroicons/react": "^2.0.18",
    "@remix-run/css-bundle": "^2.2.0",
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "@remix-run/serve": "^2.2.0",
    "@sindresorhus/slugify": "^2.2.1",
    "@white-label/email-templates": "workspace:*",
    "@white-label/ui-core": "workspace:*",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.12.7",
    "dotenv": "^16.3.1",
    "effect": "2.0.0-next.49",
    "ioredis": "^5.3.2",
    "isbot": "^3.7.0",
    "nodemailer": "^6.9.7",
    "nprogress": "^0.2.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-flat-routes": "^0.6.2",
    "remix-typedjson": "^0.3.1",
    "uuid": "^9.0.1",
    "zapatos": "^6.1.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@remix-run/dev": "^2.2.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/bcryptjs": "^2.4.5",
    "@types/node": "^20.8.10",
    "@types/nodemailer": "^6.4.13",
    "@types/nprogress": "^0.2.2",
    "@types/pg": "^8.10.7",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@types/uuid": "^9.0.6",
    "@white-label/eslint-config-remix": "workspace:*",
    "autoprefixer": "^10.4.16",
    "dbmate": "^2.7.0",
    "tailwindcss": "^3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.1",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18"
  }
}
