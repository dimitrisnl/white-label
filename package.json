{
  "name": "white-label",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "vite build && vite build --ssr",
    "build-storybook": "storybook build",
    "dev": "vite dev",
    "format": "prettier --write .",
    "generate-schema": "node --loader ts-node/esm -r dotenv/config ./app/core/db/generate-schema.ts",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint --ext .ts,.tsx .",
    "lint-fix": "eslint --ext .ts,.tsx . --fix",
    "migrations-create": "dbmate -d './app/core/db/migrations' new",
    "migrations-rollback": "dbmate -d './app/core/db/migrations' rollback",
    "migrations-run": "dbmate -d './app/core/db/migrations' migrate",
    "migrations-status": "dbmate -d './app/core/db/migrations' status",
    "prepare": "husky install",
    "start": "remix-serve ./build/index.js",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@effect/schema": "0.60.6",
    "@heroicons/react": "^2.1.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@remix-run/css-bundle": "^2.5.1",
    "@remix-run/node": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "@remix-run/serve": "^2.5.1",
    "@sindresorhus/slugify": "^2.2.1",
    "@tanstack/react-table": "^8.11.6",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.1.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv": "^16.3.2",
    "effect": "2.1.2",
    "ioredis": "^5.3.2",
    "isbot": "^4.4.0",
    "nodemailer": "^6.9.8",
    "nprogress": "^0.2.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-flat-routes": "^0.6.4",
    "remix-typedjson": "^0.4.1",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.1",
    "uuid": "^9.0.1",
    "zapatos": "^6.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.5.0",
    "@commitlint/config-conventional": "^18.5.0",
    "@faker-js/faker": "^8.3.1",
    "@remix-run/dev": "^2.5.1",
    "@storybook/addon-essentials": "^7.6.9",
    "@storybook/addon-interactions": "^7.6.9",
    "@storybook/addon-links": "^7.6.9",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.9",
    "@storybook/react": "^7.6.9",
    "@storybook/react-vite": "^7.6.9",
    "@storybook/testing-library": "^0.2.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.11.5",
    "@types/nodemailer": "^6.4.14",
    "@types/nprogress": "^0.2.3",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "autoprefixer": "^10.4.17",
    "dbmate": "^2.11.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "prettier": "^3.2.4",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-packagejson": "^2.4.9",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "rimraf": "^5.0.5",
    "storybook": "^7.6.9",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.1"
  },
  "optionalDependencies": {
    "husky": "^8.0.3"
  },
  "engines": {
    "node": ">=18"
  }
}
