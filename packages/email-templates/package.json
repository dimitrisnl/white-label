{
  "name": "@white-label/email-templates",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js",
      "import": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "rimraf dist && pnpm run /^build:/",
    "build:bundle": "node ./build.js",
    "build:types": "tsc -p ./tsconfig.json",
    "format": "prettier --write .",
    "lint": "eslint --ext .ts,.tsx .",
    "preview": "email preview ./src/templates"
  },
  "dependencies": {
    "@jsx-email/all": "^2.2.0",
    "@jsx-email/render": "^1.1.0",
    "clsx": "^2.0.0"
  },
  "devDependencies": {
    "@jsx-email/cli": "^1.1.0",
    "@types/pretty": "^2.0.1",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@white-label/eslint-config-react": "workspace:*",
    "esbuild": "^0.19.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
